{
  "name": "@deskpro-apps/sage",
  "title": "Sage",
  "description": "Link your Sage customers and suppliers to the users you have in your Deskpro instance to streamline communication with users by giving you information on their invoices and payments.",
  "version": "1.0.13",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "targets": [
    {
      "target": "user_sidebar",
      "entrypoint": "index.html"
    }
  ],
  "serveUrl": "https://apps-cdn.deskpro-service.com/__name__/__version__",
  "secrets": "",
  "appStoreUrl": "https://www.deskpro.com/product-embed/apps/sage",
  "entityAssociations": {
    "linkedSageContact": {
      "entity": "person",
      "type": "external_id",
      "title": "Linked Sage Contact"
    }
  },
  "settings": {
    "use_deskpro_saas": {
      "title": "One-Click Installation",
      "type": "boolean",
      "default": true,
      "isRequired": false,
      "isBackendOnly": false,
      "order": 5
    },
    "client_id": {
      "title": "Client ID",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": false,
      "condition": "settings.use_deskpro_saas != true",
      "order": 10
    },
    "client_secret": {
      "title": "Client Secret",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true",
      "order": 20
    },
    "callback_url": {
      "title": "Callback URL",
      "type": "app_embedded",
      "options": {
        "entrypoint": "#/admin/callback"
      },
      "condition": "settings.use_deskpro_saas != true",
      "isRequired": false,
      "isBackendOnly": true,
      "order": 30
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://oauth.accounting.sage.com/.*",
        "methods": [
          "POST"
        ],
        "timeout": 20
      },
      {
        "url": "https://api.accounting.sage.com/v3.1/.*",
        "methods": [
          "GET",
          "POST",
          "PUT"
        ],
        "timeout": 20
      }
    ]
  }
}